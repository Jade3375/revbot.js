{
  "name": "revbot.js",
  "version": "0.0.1",
  "description": "A Revolt bot client for Node.js",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jade3375/revbot.js"
  },
  "keywords": [
    "revolt",
    "bot",
    "client",
    "nodejs",
    "typescript"
  ],
  "author": "Jade3375",
  "license": "MIT License",
  "packageManager": "yarn@4.9.1",
  "devDependencies": {
    "@mxssfd/typedoc-theme": "^1.1.7",
    "@types/node": "^22.15.2",
    "@types/ws": "^8.18.1",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typedoc": "^0.28.3",
    "typedoc-plugin-extras": "^4.0.0",
    "typedoc-plugin-mdn-links": "^5.0.1",
    "typedoc-plugin-merge-modules": "^7.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "revolt-api": "^0.8.4",
    "ws": "^8.18.1"
  },
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "build": "yarn clean && tsc -p tsconfig.json",
    "start": "yarn build && yarn node --enable-source-maps dist/index.js",
    "testClient": "yarn build && yarn node --enable-source-maps dist/test.js",
    "clean": "rimraf dist",
    "test:docs": "rimraf docs && yarn typedoc --options static/typedoc.json"
  }
}
