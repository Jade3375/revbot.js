{
  "name": "revbot.js",
  "version": "0.0.1",
  "description": "A Revolt bot client for Node.js",
  "main": "dist/index.js",
  "packageManager": "yarn@4.9.1",
  "devDependencies": {
    "@types/node": "^22.15.2",
    "@types/ws": "^8.18.1",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "revolt-api": "^0.8.4",
    "ws": "^8.18.1"
  },
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "build": "yarn clean && tsc -p tsconfig.json",
    "start": "yarn build && yarn node --enable-source-maps dist/index.js",
    "testClient": "yarn build && yarn node --enable-source-maps dist/test.js",
    "clean": "rimraf dist"
  }
}
